{
  "name": "parently",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "build:apk": "eas build --profile production-apk",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "update": "eas update",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "gradle:clean": "cd android && ./gradlew clean",
    "gradle:debug": "./build-android.sh debug",
    "gradle:release": "./build-android.sh release",
    "gradle:bundle": "./build-android.sh bundle",
    "gradle:install-debug": "cd android && ./gradlew installDebug",
    "gradle:install-release": "cd android && ./gradlew installRelease",
    "prebuild": "expo prebuild --clean",
    "postinstall": "expo install --fix"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/slider": "4.5.6",
    "@shopify/react-native-skia": "2.0.0-next.4",
    "dayjs": "^1.11.13",
    "expo": "53.0.22",
    "expo-audio": "~0.4.9",
    "expo-av": "^15.1.7",
    "expo-blur": "^14.1.5",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "*",
    "expo-font": "*",
    "expo-gl": "~15.1.7",
    "expo-gl-cpp": "^11.4.0",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "^7.1.7",
    "expo-router": "~5.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-three": "^8.0.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-iconify": "1.0.2",
    "react-native-raw-bottom-sheet": "^3.0.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-worklets": "^0.4.1",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@iconify/react": "^6.0.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "keywords": [
    "parently",
    "life-admin",
    "ai-assistant",
    "family-management",
    "react-native",
    "expo"
  ],
  "author": "Parently Team",
  "license": "MIT"
}
